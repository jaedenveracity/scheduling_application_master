      <Button fx:id="loginButton" layoutX="92.0" layoutY="169.0" mnemonicParsing="false" onAction="#loginButtonClicked" prefHeight="35.0" prefWidth="103.0" text="Login" />
      <PasswordField fx:id="passwordLoginField" layoutX="95.0" layoutY="120.0" prefHeight="35.0" prefWidth="268.0" promptText="Password" />
      <Label fx:id="loginIssueLabel" alignment="CENTER" layoutX="40.0" layoutY="204.0" prefHeight="59.0" prefWidth="358.0" text="User not found, please try again" textFill="#eb0303" />
